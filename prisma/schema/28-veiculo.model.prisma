model Veiculo {
  id             String   @id @default(uuid())
  placaVeiculo   String   @unique
  placaCarreta   String?
  tipoVeiculo    String
  marcaVeiculo   String?
  modeloVeiculo  String?
  cliente        String?
  status         Boolean @default(true)
  
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  // Relacionamentos
  entradas Entrada[]

  @@map("Veiculo")
}
